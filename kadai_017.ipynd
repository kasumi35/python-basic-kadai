list = [Human('かなめ', 5), Human('たかあき', 36), Human('すぐとき', 0)]

class Human:
  def __init__(self,name,age):
    self.name = name
    self.age = age

  def check_adult(self):  
   if self.age >= 20:
    print(self.name+"は"+"大人である")
   else:
    print(self.name+"は"+"大人でない")


for i in list:
  i.check_adult()
